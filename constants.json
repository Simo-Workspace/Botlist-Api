{
    "MODELS_NAME": {
        "Bots": "addbots",
        "Users": "User",
        "Guilds": "Guilds",
        "Feedbacks": "Feedbacks"
    },
    "PORT": 80,
    "REQUIRED_BOT_PROPERTIES": ["name", "shortDescription", "longDescription", "createdAt", "approved", "avatar"],
    "ROUTES": {
        "MAIN": "/bots/:id(\\d+|@all)?(/:method(votes?))?",
        "USER": "/users/:id(\\d+)?",
        "AUTH": "/auth/callback",
        "GUILD": "/guilds/:id(\\d+)",
        "FEEDBACK": "/feedbacks/:user(\\d+)"
    },
    "REQUIRED_GUILD_PROPERTIES": ["verificationChannel", "logsChannel"]
}