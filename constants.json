{
    "MODELS_NAME": {
        "Bots": "addbots",
        "Users": "User",
        "Guilds": "Guilds",
        "Feedbacks": "Feedbacks"
    },
    "PORT": 80,
    "ROUTES": {
        "BOTS": "/api/bots/:id(\\d{16,21})?(/:method(votes?|feedbacks|exists))?(:user(\\d{16,21})?)",
        "USER": "/api/users/:id(\\d{16,21})",
        "AUTH": "/api/auth/(:method(callback|user|logout))",
        "GUILD": "/api/guilds/:id(\\d{16,21})/(:method(bots))?",
        "TOKEN": "/api/auth/token"
    },
    "REQUIRED_PROPS": {
        "FEEDBACK": [
            "stars",
            "postedAt",
            "content"
        ],
        "BOT": [
            "name",
            "shortDescription",
            "longDescription",
            "createdAt",
            "approved",
            "avatar"
        ],
        "GUILD": [
            "verificationChannel",
            "logsChannel"
        ]
    },
    "MAX_REQUESTS_PER_MIN": 25
}